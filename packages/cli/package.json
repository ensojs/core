{
  "name": "@ensojs/cli",
  "description": "CLI for the Enso framework",
  "version": "0.2.0",
  "author": "1a35e1@protonmail.com",
  "bin": {
    "enso": "./bin/run"
  },
  "bugs": "https://github.com/1a35e1/cli/issues",
  "dependencies": {
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/plugin-help": "^2",
    "tslib": "^1"
  },
  "devDependencies": {
    "@oclif/dev-cli": "1.22.0",
    "@oclif/tslint": "3.1.1",
    "@types/node": "10.14.10",
    "globby": "8.0.2",
    "ts-node": "8.3.0",
    "tslint": "5.18.0",
    "typescript": "3.5.2"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
