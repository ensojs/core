  "homepage": "https://github.com/1a35e1/cli",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "enso",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "1a35e1/cli",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "tslint -p . -t stylish",
