{
  "name": "@enso/starter-api-typeorm",
  "version": "0.3.0",
  "description": "An API, using TypeORM using the Enso framework",
  "main": "src/server.ts",
  "scripts": {
    "start": "DEBUG=* ts-node -r tsconfig-paths/register src/server.ts",
    "test": "jest",
    "migration:create": "ts-node -r tsconfig-paths/register ../../node_modules/typeorm/cli.js migration:create",
    "migration:revert": "ts-node -r tsconfig-paths/register ../../node_modules/typeorm/cli.js migration:revert",
    "migration:run": "ts-node -r tsconfig-paths/register ../../node_modules/typeorm/cli.js migration:run",
    "schema:drop": "ts-node -r tsconfig-paths/register ../../node_modules/typeorm/cli.js schema:drop"
  },
  "license": "MIT",
  "dependencies": {
    "class-transformer": "^0.2.0",
    "class-transformer-validator": "^0.6.0",
    "dotenv-safe": "^6.1.0",
    "inversify": "^5.0.1",
    "inversify-koa": "^1.0.3",
    "koa-router": "^7.4.0",
    "reflect-metadata": "^0.1.12",
    "supertest": "^3.3.0",
    "ts-node": "^8.0.2",
    "typeorm": "^0.2.14"
  },
  "devDependencies": {
    "@types/dotenv-safe": "^5.0.3",
    "@types/jest": "^23.3.10",
    "@types/koa-router": "^7.0.32",
    "@types/supertest": "^2.0.7",
    "jest-cli": "^23.6.0",
    "ts-jest": "^23.10.5",
    "tsconfig-paths": "^3.6.0"
  },
  "peerDependencies": {
    "inversify": "^5.0.1",
    "tslib": "^1.9.3",
    "typeorm": "^0.2.7"
  }
}
